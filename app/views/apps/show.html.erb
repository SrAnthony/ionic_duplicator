<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @app.name %>
</p>

<p>
  <strong>Id android:</strong>
  <%= @app.id_android %>
</p>

<p>
  <strong>Id ios:</strong>
  <%= @app.id_ios %>
</p>

<p>
  <strong>Version:</strong>
  <%= @app.version %>
</p>

<p>
  <strong>Color primary:</strong>
  <%= @app.color_primary %>
</p>

<p>
  <strong>Color secondary:</strong>
  <%= @app.color_secondary %>
</p>

<div id="results" class="hide">
  <h3>Results</h3>
  <pre></pre>
</div>

<%= link_to 'Edit', edit_app_path(@app), class: 'ui button' %> |
<%= link_to 'Back', apps_path, class: 'ui button' %> |
<%= link_to 'Create Android App', create_app_path(@app, 'android'), remote: :true, class: 'ui button will-load' %> |
<%= link_to 'Create iOS App', create_app_path(@app, 'ios'), remote: :true, class: 'ui button will-load' %>
